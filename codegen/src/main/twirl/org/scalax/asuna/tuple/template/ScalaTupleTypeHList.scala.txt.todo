        override type H = (@for(ii <- 1 to i) { E@{ii}#H @if(ii < i) { , } })
        override type T = ScalaTupleTypeHList@{i}[@for(ii <- 1 to i) { E@{ii}#T @if(ii < i) { , } }]
    }
}